<h1>All posts</h1>

<ul>
  <% posts.each do |post| %>
  <li>
    <a href="/posts/<%=post.id%>"><%= post.title %></a>
    <a href="/post/<%=post.id%>/edit">Edit</a> -
    <a href="/post/<%=post.id%>?_method=delete" method="delete">Delete</a>
  </li>
  <% end %>
</ul>

<a href="/posts/new"> Create new post </a>